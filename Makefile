APP:=mprom
CPARGS=""

.PHONY: help
help:
	# -----------------------------------------------------------------------------
	# Targets:
	#
	# ** Local Commands **
	# 	build:          build service locally
	#
	# ** Misc **
	# 	selftest:	run through all make targets
	#
	# end.
	# -----------------------------------------------------------------------------

# -----------------------------------------------------------------------------
#  LOCAL COMMANDS
# -----------------------------------------------------------------------------
.PHONY: build
build:  
	 rebar3 do clean, compile

# -----------------------------------------------------------------------------
#  SELFTEST
# -----------------------------------------------------------------------------

.PHONY: selftest
selftest: build 
